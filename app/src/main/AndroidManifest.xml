<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.xhq.demo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:name=".HomeApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="BASE_URL"
            android:value="${BASE_URL}" />
        <meta-data
            android:name="MW_APPID"
            android:value="OJHP6BEGOA4ESYQMSPOHRPZ45WZ7QGZ9" />

        <meta-data
            android:name="BASE_SHARE_URL"
            android:value="${BASE_SHARE_URL}" />

        <meta-data
            android:name="APPLY_NAME"
            android:value="${app_name}" />
        <meta-data
            android:name="com.netease.nim.appKey"
            android:value="${YX_APPKEY}" />

        <meta-data
            android:name="CUR_ENV"
            android:value="${CUR_ENV}" />


        <activity android:name="com.xhq.demo.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>