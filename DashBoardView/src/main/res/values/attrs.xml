<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DashBoardView">
        <attr name="arcRadius" format="dimension"/>
        <attr name="startAngle" format="integer"/>
        <attr name="sweepAngle" format="integer"/>
        <attr name="arcColor" format="color"/>
        <attr name="ribbonWidth" format="dimension"/>
        <attr name="ribbonMode">
            <enum name="normal" value="0"/>
            <enum name="inner" value="1"/>
            <enum name="outer" value="2"/>
        </attr>
        <attr name="pieceTotalCount" format="integer"/>
        <attr name="minScaleCountInOnePiece" format="integer"/>
        <attr name="scaleValueTextSize" format="dimension"/>
        <attr name="ScaleValueTextColor" format="color"/>
        <attr name="minScaleValue" format="integer"/>
        <attr name="maxScaleValue" format="integer"/>
        <attr name="currentScaleValue" format="float"/>
        <attr name="pointerRadius" format="dimension"/>
        <attr name="centerCircleRadius" format="dimension"/>
        <attr name="unitText" format="string"/>
        <attr name="unitTextSize" format="string"/>
        <attr name="unitTextRadius" format="dimension"/>
        <attr name="bgColor" format="color"/>
    </declare-styleable>
</resources>